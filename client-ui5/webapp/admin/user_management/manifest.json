{
    "_version": "1.59.0",
    "sap.app": {
        "id": "user.management",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "Gestión de Usuarios",
        "description": "App para gestión de usuarios, roles y perfiles",
        "applicationVersion": {
            "version": "1.0.0"
        }
    },
    "sap.ui5": {
        "resources": {
            "css": [
                {
                    "uri": "../../css/style.css"
                }
            ]
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "user.management.i18n.i18n"
                }
            }
        },
        "rootView": {
            "viewName": "user.management.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.tnt": {},
                "sap.ui.layout": {}
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "controlId": "app",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                    "target": "notFound"
                }
            },
            "routes": [
                {
                    "name": "home",
                    "pattern": "",
                    "target": "home"
                },
                {
                    "name": "users",
                    "pattern": "users",
                    "target": "home"
                },
                {
                    "name": "userDetail",
                    "pattern": "users/{userId}",
                    "target": "home"
                },
                {
                    "name": "roles",
                    "pattern": "roles",
                    "target": "home"
                },
                {
                    "name": "roleDetail",
                    "pattern": "roles/{roleId}",
                    "target": "home"
                },
                {
                    "name": "auth",
                    "pattern": "auth",
                    "target": "home"
                },
                {
                    "name": "profiles",
                    "pattern": "profiles",
                    "target": "home"
                },
                {
                    "name": "rolProfileDetail",
                    "pattern": "profiles/{profileId}",
                    "target": "home"
                },
                {
                    "name": "invoiceCountryList",
                    "pattern": "countries",
                    "target": "home"
                },
                {
                    "name": "invoiceCountryDetail",
                    "pattern": "countries/{countryId}",
                    "target": "home"
                }
            ],
            "targets": {
                "home": {
                    "viewId": "home",
                    "viewName": "user.management.view.Main"
                },
                "invoiceCountryList": {
                    "viewId": "invoiceCountryList",
                    "viewName": "invoice.app.view.InvoiceCountriesList"
                },
                "invoiceCountryDetail": {
                    "viewId": "invoiceCountryDetail",
                    "viewName": "invoice.app.view.InvoiceCountryDetail"
                },
                "notFound": {
                    "viewId": "notFound",
                    "viewName": "user.management.view.NotFound"
                }
            }
        }
    }
}