{
    "_version": "1.59.0",
    "sap.app": {
        "id": "invoice.app",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "invoiceService": {
                "uri": "/api/invoices/",
                "type": "JSON"
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "invoice.app.view.Shell",
            "type": "XML",
            "id": "shell"
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {},
                "sap.tnt": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "invoice.app.i18n.i18n"
                }
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "invoice.app.view",
                "controlId": "shellNavContainer",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                    "target": "notFound"
                }
            },
            "routes": [
                {
                    "name": "home",
                    "pattern": "",
                    "target": "home"
                },
                {
                    "name": "dashboard",
                    "pattern": "dashboard",
                    "target": "dashboard"
                },
                {
                    "name": "invoiceList",
                    "pattern": "invoices/:tipo:",
                    "target": "invoiceList"
                },
                {
                    "name": "invoiceNew",
                    "pattern": "invoice/new/:tipo:",
                    "target": "invoiceForm"
                },
                {
                    "name": "invoiceEdit",
                    "pattern": "invoice/{invoiceId}/edit",
                    "target": "invoiceForm"
                },
                {
                    "name": "invoiceDetail",
                    "pattern": "invoice/{invoiceId}",
                    "target": "invoiceDetail"
                },
                {
                    "name": "issuerManager",
                    "pattern": "issuers",
                    "target": "issuerManager"
                },
                {
                    "name": "receiverManager",
                    "pattern": "receivers",
                    "target": "receiverManager"
                },
                {
                    "name": "catalogList",
                    "pattern": "catalog",
                    "target": "catalogList"
                },
                {
                    "name": "catalogNew",
                    "pattern": "catalog/new",
                    "target": "catalogDetail"
                },
                {
                    "name": "catalogDetail",
                    "pattern": "catalog/{productId}",
                    "target": "catalogDetail"
                },
                {
                    "name": "workflowList",
                    "pattern": "workflows",
                    "target": "workflowList"
                },
                {
                    "name": "workflowNew",
                    "pattern": "workflow/new",
                    "target": "workflowDesigner"
                },
                {
                    "name": "workflowDesigner",
                    "pattern": "workflow/{workflowId}",
                    "target": "workflowDesigner"
                },
                {
                    "name": "facturaE",
                    "pattern": "invoice/{invoiceId}/facturae",
                    "target": "facturaE"
                },
                {
                    "name": "ubl21",
                    "pattern": "invoice/{invoiceId}/ubl21",
                    "target": "ubl21"
                },
                {
                    "name": "scanInvoice",
                    "pattern": "scan-invoice",
                    "target": "scanInvoice"
                },
                {
                    "name": "originList",
                    "pattern": "admin/origins",
                    "target": "originList"
                },
                {
                    "name": "apiDocs",
                    "pattern": "api-docs",
                    "target": "apiDocs"
                },
                {
                    "name": "auditLog",
                    "pattern": "audit-log",
                    "target": "auditLog"
                },
                {
                    "name": "xmlMapping",
                    "pattern": "mapping/xml",
                    "target": "xmlMapping"
                }
            ],
            "targets": {
                "xmlMapping": {
                    "viewId": "xmlMapping",
                    "viewName": "XmlMapping",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "home": {
                    "viewId": "home",
                    "viewName": "Home",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "dashboard": {
                    "viewId": "dashboard",
                    "viewName": "Dashboard",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "invoiceList": {
                    "viewId": "invoiceList",
                    "viewName": "InvoiceList",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "invoiceDetail": {
                    "viewId": "invoiceDetail",
                    "viewName": "InvoiceDetail",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "invoiceForm": {
                    "viewId": "invoiceForm",
                    "viewName": "InvoiceForm",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "issuerManager": {
                    "viewId": "issuerManager",
                    "viewName": "IssuerManager",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "receiverManager": {
                    "viewId": "receiverManager",
                    "viewName": "ReceiverManager",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "catalogList": {
                    "viewId": "catalogList",
                    "viewName": "CatalogList",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "catalogDetail": {
                    "viewId": "catalogDetail",
                    "viewName": "CatalogDetail",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "workflowList": {
                    "viewId": "workflowList",
                    "viewName": "WorkflowList",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "workflowDesigner": {
                    "viewId": "workflowDesigner",
                    "viewName": "WorkflowDesigner",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "facturaE": {
                    "viewId": "facturaE",
                    "viewName": "FacturaE",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "ubl21": {
                    "viewId": "ubl21",
                    "viewName": "FacturaUBL",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "scanInvoice": {
                    "viewId": "scanInvoice",
                    "viewName": "ScanInvoice",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "originList": {
                    "viewId": "originList",
                    "viewName": "OriginList",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "apiDocs": {
                    "viewId": "apiDocs",
                    "viewName": "ApiDocs",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "notFound": {
                    "viewId": "notFound",
                    "viewName": "NotFound"
                },
                "auditLog": {
                    "viewId": "auditLog",
                    "viewName": "AuditLog",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                }
            }
        }
    }
}