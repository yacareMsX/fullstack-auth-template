{
    "_version": "1.58.0",
    "sap.app": {
        "id": "invoice.app",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "version": "1.40.12"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.108.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.tnt": {},
                "sap.ui.layout": {},
                "sap.f": {},
                "sap.uxap": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "invoice.app.i18n.i18n"
                }
            },
            "ui": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "headerTitle": "Compliance Hub",
                    "themeIcon": "sap-icon://light-mode",
                    "shellSidebarExpanded": true,
                    "shellSidebarVisible": true
                }
            },
            "security": {
                "type": "sap.ui.model.json.JSONModel",
                 "uri": "/api/auth/me"
            }
        },
        "rootView": {
            "viewName": "invoice.app.view.Shell",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "invoice.app.view",
                "controlId": "shellNavContainer",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "name": "home",
                    "pattern": "",
                    "target": "home"
                },
                {
                    "name": "invoices",
                    "pattern": "invoices",
                    "target": "invoiceList"
                },
                {
                    "name": "issueInvoices",
                    "pattern": "invoices/issued",
                    "target": "issueInvoices"
                },
                {
                    "name": "receiptInvoices",
                    "pattern": "invoices/received",
                    "target": "receiptInvoices"
                },
                {
                    "name": "invoiceDetail",
                    "pattern": "invoices/{invoiceId}",
                    "target": "invoiceDetail"
                },
                {
                    "name": "dashboard",
                    "pattern": "dashboard",
                    "target": "dashboard"
                },
                {
                    "name": "catalog",
                    "pattern": "catalog",
                    "target": "catalog"
                },
                {
                    "name": "scanInvoice",
                    "pattern": "scan",
                    "target": "scanInvoice"
                },
                {
                    "name": "userManagement",
                    "pattern": "admin/users",
                    "target": "userManagement"
                },
                {
                    "name": "workflowDesigner",
                    "pattern": "workflows/designer",
                    "target": "workflowDesigner"
                },
                {
                    "name": "workflowList",
                    "pattern": "workflows/list",
                    "target": "workflowList"
                },
                {
                    "name": "issuerManager",
                    "pattern": "config/issuers",
                    "target": "issuerManager"
                },
                {
                    "name": "receiverManager",
                    "pattern": "config/receivers",
                    "target": "receiverManager"
                },
                {
                    "name": "invoiceCountries",
                    "pattern": "config/invoice-countries",
                    "target": "invoiceCountries"
                },
                {
                    "name": "originList",
                    "pattern": "admin/origins",
                    "target": "originList"
                },
                {
                    "name": "certificateList",
                    "pattern": "certificates",
                    "target": "certificateLayout"
                },
                {
                    "name": "documentationXml",
                    "pattern": "documentation/xml",
                    "target": "documentationXml"
                },
                {
                    "name": "documentationDetail",
                    "pattern": "documentation/xml/{id}",
                    "target": "documentationDetail"
                },
                {
                    "name": "invoiceCountryList",
                    "pattern": "config/countries",
                    "target": "invoiceCountryList"
                },
                {
                    "name": "statutoryModels",
                    "pattern": "statutory/models",
                    "target": "statutoryModels"
                },
                {
                    "name": "model303",
                    "pattern": "statutory/models/303",
                    "target": "model303"
                },
                {
                    "name": "roleManagement",
                    "pattern": "admin/roles",
                    "target": "roleManagement"
                },
                {
                    "name": "authObjectsManagement",
                    "pattern": "admin/auth-objects",
                    "target": "authObjectsManagement"
                },
                {
                    "name": "rolProfilesManagement",
                    "pattern": "admin/rol-profiles",
                    "target": "rolProfilesManagement"
                },
                {
                    "name": "auditLog",
                    "pattern": "audit",
                    "target": "auditLog"
                }
            ],
            "targets": {
                "home": {
                    "viewId": "home",
                    "viewName": "Home",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "invoiceList": {
                    "viewId": "invoiceList",
                    "viewName": "InvoiceList",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "invoiceDetail": {
                    "viewId": "invoiceDetail",
                    "viewName": "InvoiceDetail",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "dashboard": {
                    "viewId": "dashboard",
                    "viewName": "Dashboard",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "catalog": {
                    "viewId": "catalog",
                    "viewName": "Catalog",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "scanInvoice": {
                    "viewId": "scanInvoice",
                    "viewName": "ScanInvoice",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "userManagement": {
                    "viewId": "userManagement",
                    "viewName": "admin.UserManagement",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "workflowDesigner": {
                    "viewId": "workflowDesigner",
                    "viewName": "WorkflowDesigner",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "workflowList": {
                    "viewId": "workflowList",
                    "viewName": "WorkflowList",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "issuerManager": {
                    "viewId": "issuerManager",
                    "viewName": "IssuerManager",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "receiverManager": {
                    "viewId": "receiverManager",
                    "viewName": "ReceiverManager",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "invoiceCountries": {
                    "viewId": "invoiceCountries",
                    "viewName": "InvoiceCountries",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "originList": {
                    "viewId": "originList",
                    "viewName": "admin.Origenes",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "certificateLayout": {
                    "viewId": "certificateLayout",
                    "viewName": "certificates.CertificateLayout",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "certificateList": {
                    "viewId": "certificateList",
                    "viewName": "certificates.CertificateList",
                    "controlId": "certificateNavContainer",
                    "controlAggregation": "pages",
                    "parent": "certificateLayout"
                },
                "certificateDetail": {
                    "viewId": "certificateDetail",
                    "viewName": "certificates.CertificateDetail",
                    "controlId": "certificateNavContainer",
                    "controlAggregation": "pages",
                    "parent": "certificateLayout"
                },
                "documentationXml": {
                    "viewId": "documentationXml",
                    "viewName": "DocumentationXml",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "documentationDetail": {
                    "viewId": "documentationDetail",
                    "viewName": "DocumentationDetail",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "invoiceCountryList": {
                    "viewId": "invoiceCountryList",
                    "viewName": "InvoiceCountries",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "statutoryModels": {
                    "viewId": "statutoryModels",
                    "viewName": "statutory.Models",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "model303": {
                    "viewId": "model303",
                    "viewName": "statutory.Model303",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                 "roleManagement": {
                    "viewId": "roleManagement",
                    "viewName": "admin.RoleManagement",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "authObjectsManagement": {
                    "viewId": "authObjectsManagement",
                    "viewName": "admin.AuthObjectsManagement",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                "rolProfilesManagement": {
                    "viewId": "rolProfilesManagement",
                    "viewName": "admin.RolProfilesManagement",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                },
                 "auditLog": {
                    "viewId": "auditLog",
                    "viewName": "AuditLog",
                    "controlId": "shellNavContainer",
                    "controlAggregation": "pages"
                }

            }
        }
    }
}
